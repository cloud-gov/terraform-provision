        "iam:List*",
        "iam:GenerateCredentialReport",
        "kinesis:ListStreams",
        "kinesis:DescribeStream",
        "kinesis:GetShardIterator",
        "kinesis:GetRecords",
        "kms:Get*",
        "kms:List*",
        "lambda:ListFunctions",
        "rds:Describe*",
